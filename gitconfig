[core]
	editor = vim
[alias]
	a = add
	ap = add --patch
	b = branch --verbose
	br = branch --all --verbose
	c = commit
	ca = commit --amend
	cb = checkout -b
	co = checkout
	cp = checkout --patch
	ct = checkout --track
	d = diff
	dc = diff --cached
	f = fetch
	g = grep -ni
	l = log
	ll = log --oneline
	lo = log --oneline --max-count=10
	ls = log --graph --simplify-by-decoration --pretty=format:'%d' --all
	lp = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen((%cr) %C(bold blue)%Creset' --abbrev-commit
	up = remote update
	pgm = push github master
	pr = pull --rebase
	r = reset
	ra = rebase --abort
	rb = rebase
	rc = rebase --continue
	ri = rebase --interactive
	rp = reset --patch
	s = status
	sh = show
	smi = submodule update --init
	smu = submodule foreach git pull origin master
	tc = tag --contains
